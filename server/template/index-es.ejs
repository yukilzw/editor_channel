<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <% if (id === 'app') { %>
    <meta name="description" content="<%= keys %> - <%= desc %>">
    <% } %>
    <title><%= title %></title>
    <link rel="shortcut icon" href="/template/favicon.ico"/>
    <% css.forEach(function(url) { %>
    <link href="<%- url %>" rel="stylesheet">
    <% }); %>
    <script>window.comp = {};</script>
</head>
<body>
<div id="<%= id %>"></div>
</body>
<% js.forEach(function(url) { %>
<script type="text/javascript" src="<%- url %>"></script>
<% }); %>
</html>